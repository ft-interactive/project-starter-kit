  "dependencies": {
    "@financial-times/g-components": "^9.0.1",
    "@financial-times/o-colors": "^6.6.2",
    "@financial-times/o-grid": "^6.1.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "type": "module"
