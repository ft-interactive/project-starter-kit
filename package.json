    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "mocha": "^10.2.0",
    "pa11y": "^6.2.3",
    "prettier": "^3.0.3",
    "sass": "^1.68.0",
    "vite": "^4.4.9",
    "vite-plugin-ejs": "^1.6.4",
    "vite-plugin-generate-file": "^0.0.4"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "optionalDependencies": {
    "@financial-times/g-deploy": "^3.2.0",
    "@financial-times/vvc": "github:financial-times/visual-vocabulary-components#prerelease",
    "react-snap": "^1.23.0"
  },
  "private": true,
  "scripts": {
    "build": "vite build",
    "a11y": "pa11y $(g-deploy --get-commit-url)",
