{
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@financial-times/g-deploy": "^3.0.8",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.0-beta.4",
    "bower": "^1.8.0",
    "chai": "^4.1.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "csv-loader": "^3.0.2",
    "d3-time-format": "^2.0.5",
    "dotenv": "^6.1.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mdx": "^1.4.6",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "generate-json-webpack-plugin": "^0.3.1",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.2.0",
    "jsdom": "^11.11.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.1.1",
    "node-sass": "^4.9.2",
    "pa11y": "^5.0.3",
    "papaparse": "^4.6.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.0.6",
    "postcss-preset-env": "^6.5.0",
    "postcss-scss": "^1.0.2",
    "prettier": "^1.18.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "undom": "^0.4.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5.3.x"
  },
  "private": true,
  "scripts": {
    "build": "webpack -p --env=production",
    "a11y": "pa11y $(g-deploy --get-commit-url)",
    "a11y:preview": "pa11y $(g-deploy --preview --get-commit-url)",
    "a11y:local": "pa11y localhost:8080",
    "deploy": "g-deploy --assets-prefix=https://ig.ft.com/v2/__assets/",
    "postinstall": "bower install --allow-root",
    "pretest": "npm run build",
    "test": "mocha ./test/**/*.spec.js",
    "start": "webpack-dev-server -r dotenv/config",
    "lint": "eslint server/**/*.js client/**/*.js test/**/*.js",
    "lint:fix": "eslint --fix server/**/*.js client/**/*.js test/**/*.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@financial-times/g-components": "^2.2.2",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet-async": "0.1.0-alpha",
    "react-markdown": "^3.4.1"
  }
}
